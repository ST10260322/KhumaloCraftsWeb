@{
    ViewData["Title"] = "Products";
}

<h1>
    Products
</h1>

<img src="data:image/jpeg;base64," />

<p>
    To add new products in the database add the details of the product into the system:
</p>

<body>
    <form action="Product" method="post">
        <input type="text" name="productName" placeholder="Product Name" required>
        <input type="number" name="productPrice" placeholder="Product Price" required>
        <input type="text" name="Category" placeholder="Category" required>
        <input type="text" name="availability" placeholder="Availability" required>
       
        <button type="submit">Submit</button>
    </form>
</body>

